		"css": ".social {\n margin-right: 8px !important;\n}\n\n@media only screen and (max-width: 600px) {\n .site-title {\n     font-size:12px !important;\n}\n\n.wp-block-navigation__container {\n  margin-left: 8px;\n\n}\n\n.top-header-nav {\n  padding-left: 8px\n}\n\n.header-seperator {\n   display: none;\n}\n.header-cover .wp-block-cover__image-background {\n  object-fit:fill;\n}\n\nbody {\n  --wp--style--root--padding-right: 0;\n--wp--style--root--padding-left: 0;\n}\nhtml, body {\n overflow-x: hidden;\n}\n\n.primary-menu-navigation {\n  margin-left: 8px !important;\n }\n.wp-block-navigation__responsive-container.is-menu-open .wp-block-navigation__responsive-container-content {\n  padding-top: 8px !important;\n}\n.wp-block-navigation__responsive-container-close {\n  margin-right: 8px !important;\n}\n\n}\n.home-service-show-more {\n   display: none;\n}\n.homepage-cover-section:hover .home-service-show-more {\n position: absolute;\n    width: 100%;\n    top: 50%;\n    transform: translate(-50%, -50%);\ndisplay: block;\nleft: 50%\n}\n.homepage-cover-section:hover \n.has-background-dim{\n  opacity: 0.5 !important;\n}\n.homepage-cover-section:hover .home-service-title {\ndisplay: none;\n}\n\na:where(:not(.wp-element-button)) {\n  font-size: 24px\n}\n\n.wp-block-pullquote {\n padding-top: 0;\n padding-bottom: 20px;\n}",
